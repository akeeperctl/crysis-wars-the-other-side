	<Strategy name="GameStart0">
		<SelectConditions>
			<Condition type="Game" name="GameStatus" conditional1="GameStatus" operator="Equal" strvalue="GameStart"/>
		</SelectConditions>
		<Settings>
			<param name="vehicleUseDistance" value="30"/> 
			<param name="aggression" value="100"/> 
			<param name="numberOfAttacks" value="1"/>
			<param name="numberOfDefences" value="0"/>
			<param name="timeLimit" value="60"/>
		</Settings>
		<Goals>
			<Goal type="Count" name="CapturedAreasWhileInStrategy" relationship="Friendly" conditional1="Areas" conditional2="ControlPoint" operator="Equal" fvalue="1"/>
		</Goals>
		<AreaStrategyPriorities>
			<Area flag="Base" priority="20" status="Any"/>
			<Area flag="ControlPoint" priority="200" status="Any"/>
		</AreaStrategyPriorities>
	</Strategy>
